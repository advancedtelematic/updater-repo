	<!-- BSP layers -->
	<project path="meta-raspberrypi" name="meta-raspberrypi" remote="yocto" revision="28d4404f89eb59d406b4976c0e3f5ca19137ba74" />
	<project name="meta-intel" path="meta-intel" remote="yocto" revision="morty" />
